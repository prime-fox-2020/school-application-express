<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Teacher List</title>
</head>
<body>
    <h1> List of Data Teachers</h1>

    <% if (dataTeacher) { %>
        <table border="1">
            <tr>
                <th>ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
            </tr>
            <% dataTeacher.forEach(item => { %>
                <tr>
                    <td><%= item.id %></td>
                    <td><%= item.first_name %></td>
                    <td><%= item.last_name %></td>
                    <td><%= item.email %></td>
                </tr>
            <% }) %>
        </table>
    <% }%>
</body>
</html>