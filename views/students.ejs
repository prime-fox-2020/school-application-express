<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>List of Data Student</h1>

    <% if (dataStudent) { %>
        <table border="1">
            <tr>
                <th>ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
                <th>Option</th>
            </tr>
            <% dataStudent.forEach(item => { %>
                <tr>
                    <td><%= item.id %></td>
                    <td><%= item.first_name %></td>
                    <td><%= item.last_name %></td>
                    <td><%= item.email %></td>
                    <td>
                        <a href="/students/edit/<% item.id %>">Edit</a>
                        |
                        <a href="/students/delete/<% item.id %>">Delete</a>
                    </td>
                </tr>
            <% }) %>
        </table>
    <% }%>
</body>
</html>