<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Students Data</title>
</head>

<body>
    <h1>Students Data</h1>
    <table>
        <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Gender</th>
            <th>Birth Date</th>
        </tr>
        <% for(let i = 0; i<data.length; i++) { %>
        <tr>
            <td><%= data[i].id %></td>
            <td><%= data[i].first_name %></td>
            <td><%= data[i].last_name %></td>
            <td><%= data[i].email %></td>
            <td><%= data[i].gender %></td>
            <td><%= data[i].birth_date %></td>
        </tr>
        <% } %>
    </table>


</body>

</html>